// kovan
1. parity --geth --chain kovan --force-ui --reseal-min-period 0 --jsonrpc-cors http://localhost

2. parity ui

2. truffle migrate --network kovan / --reset

// rinkeby
1. geth --rinkeby --rpc --rpcapi db,eth,net,web3,personal
2.  geth --rinkeby --rpc --rpcapi db,eth,net,web3,personal --unlock="0x0FC339D948a258AB986B71EF7fd78530a824D63a"


